alias whatsmyip="ipconfig getifaddr en0"
alias httpserver="python3 -m http.server"
alias httpserver2="python3 -m http.server 8001"
alias editdotfiles="cd $HOME/.dotfiles && code ."
alias reloadzsh="echo 'reloading zsh ...' && source $HOME/.zshrc && echo 'done'"

# hedia dev services
alias developers-service="cd ~/hedia/services/developers-service && npm install && open http://localhost:6869 && npm run dev"
